{
  "name": "@digvan/c-v-3",
  "version": "0.0.9",
  "description": "Reusable UI components for Vue.js",
  "main": "dist/c-v-3.umd.js",
  "module": "dist/c-v-3.es.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/c-v-3.es.js",
      "require": "./dist/c-v-3.umd.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "pub": "npm run build && npm version patch && npm publish --access=public --registry=https://registry.npmjs.org"
  },
  "keywords": [
    "vue3",
    "component"
  ],
  "author": "511",
  "license": "MIT",
  "dependencies": {
    "vue": "^3.2.31"
  },
  "peerDependencies": {
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^8.0.0",
    "cssnano": "^5.0.17",
    "rollup": "^2.68.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-vue": "^6.0.0"
  },
  "browserslist": [
    "defaults",
    "not ie < 8",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ]
}
